#include <iostream>
#include <vector>
#include <algorithm> //for find
using namespace std;


int main()
{
    int n=0,length=0;//n組測試資料,length for train
    int temp=0,sum=0;
    vector<int> train;

    cin >> n;

    for(int i=0;i<n;i++)
    {
        train.clear();
        cin >> length;
        for(int j=0;j<length;j++)
        {
            cin >> temp;
            train.push_back(temp);  //temp for int
        }
        sum=0;
        for(int k=0;k<length;k++)
        {
            temp=0;
            if(train[k]!=k+1)
            {
                auto it = find(train.begin(), train.end(), k+1); //it for iterator not a integer
                temp = static_cast<int>(it - train.begin()) - k; //(temp - k) for pos

                train.erase(train.begin()+temp+k);               //train change
                train.insert(train.begin()+k,k+1);
                              
                sum+=temp;                                       //temp for count
            }
        }
        cout << "Optimal train swapping takes " << sum << " swaps." << endl;

    }


    cin >> n;

    getchar();
    getchar();
    return 0;
}
