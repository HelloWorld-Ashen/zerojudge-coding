#include <iostream>
#include <string>
using namespace std;

int main()
{
    string s, res;
    cin >> s;

    res.resize(s.size());

    for(int i=0;i<int(s.size());i++)
    {
        res[i]=s[int(s.size())-i-1];
    }

    if(res==s) cout << "yes\n";
    else cout << "no\n";

    getchar();
    getchar();
    return 0;

}
