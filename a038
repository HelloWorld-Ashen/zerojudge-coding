#include <iostream>
#include <vector>
using namespace std;

int main()
{
    string x;
    cin >> x;

    vector<int> num;

    
    for (int i = int(x.length()) - 1; i >= 0; i--)
        num.push_back(int(x[i] - '0'));

    
    while (!num.empty() && num.back() == 0)
        num.pop_back();  

    
    while (!num.empty() && num.front() == 0)
        num.erase(num.begin());

    
    if (num.empty())
    {
        cout << 0;
    }
    else
    {
        
        for (int i = 0; i < num.size(); i++)
            cout << num[i];
    }

    getchar();
    getchar();
    return 0;
}
